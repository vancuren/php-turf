{
    "name": "vancuren/php-turf",
    "description": "A PHP library for geospatial analysis similar to Turf.js",
    "keywords": ["turf", "turf.js", "geospatial", "analysis", "gis", "geojson", "geometry", "math", "spatial", "topology", "vector"],
    "license": "AGPL-3.0-or-later",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Vancuren\\PhpTurf\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Russell Van Curen"
        }
    ],
    "require": {
        "php": "^7.1 || ^8.0"
    },    
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit tests"
        ]
    },  
    "minimum-stability": "stable"
}